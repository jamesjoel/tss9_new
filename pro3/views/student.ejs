<html>

<head>
    <title><%= title %></title>
    <style>
        body {
            background-color: #BABABA;
        }
    </style>
</head>

<body>
    <h1>Student</h1>
    <table align="center" border="1" cellspaceing="0">
        <tr>
            <th>S.No.</th>
            <th>Name</th>
            <th>Age</th>
        </tr>
        <% data.forEach(function(x, i){ %>
            <tr>
                <td>
                    <%= i+1 %>
                </td>
                <td>
                    <%= x.name %>
                </td>
                <td>
                    <%= x.age %>
                </td>
            </tr>

            <% }) %>



    </table>

</body>

</html>